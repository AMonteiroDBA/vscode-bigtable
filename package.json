{
  "name": "vscode-bigtable",
  "displayName": "vscode-bigtable",
  "description": "A VSCode extension for Google Bigtable",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/a7ul/vscode-bigtable.git"
  },
  "publisher": "a7ul",
  "icon": "resources/icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vscodeBigtable_views_gcpProjectsList",
    "onView:vscodeBigtable_views_addedTableList",
    "onCommand:vscodeBigtable_command_addTable",
    "onCommand:vscodeBigtable_command_openTable"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscodeBigtable_command_openTable",
        "title": "Open Bigtable"
      },
      {
        "command": "vscodeBigtable_command_addTable",
        "title": "Add Bigtable",
        "enablement": "false"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscodeBigtable_viewContainers_activitybar",
          "title": "Bigtable",
          "icon": "resources/bigtable.svg"
        }
      ]
    },
    "views": {
      "vscodeBigtable_viewContainers_activitybar": [
        {
          "id": "vscodeBigtable_views_addedTableList",
          "name": "My Tables"
        },
        {
          "id": "vscodeBigtable_views_gcpProjectsList",
          "name": "GCP Projects"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && make all",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@google-cloud/bigtable": "^3.12.0",
    "@google-cloud/resource-manager": "^3.2.0",
    "cross-fetch": "^3.1.5",
    "react-json-view": "^1.21.3"
  }
}
